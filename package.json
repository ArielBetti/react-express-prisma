  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "4.17.16",
    "@types/node": "18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.22",
    "prisma": "^4.12.0",
    "ts-node": "10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
